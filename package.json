{
  "name": "eterna-data-aggregator",
  "version": "1.0.0",
  "description": "Real-time meme coin data aggregation service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cd frontend && npm run build",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "dev": "ts-node src/index.ts",
    "dev:frontend": "cd frontend && npm start",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "install:all": "npm install && cd frontend && npm install"
  },
  "keywords": ["defi", "meme-coin", "aggregator", "websocket", "real-time"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "redis": "^4.6.5",
    "axios": "^1.6.7",
    "axios-retry": "^3.9.1",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/cors": "^2.8.17",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "supertest": "^6.3.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5"
  }
}
